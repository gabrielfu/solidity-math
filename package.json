{
  "name": "solidity-math",
  "description": "Solidity number types in Javascript",
  "scripts": {
    "compile": "npm run lint && rm -rf dist/lib.commonjs && rm -rf dist/lib.es6 && tsc && tsc --build tsconfig.commonjs.json",
    "compile-windows": "npm run lint && (if exist dist\\lib.commonjs rmdir /s /q dist\\lib.commonjs) && (if exist dist\\lib.es6 rmdir /s /q dist\\lib.es6) && tsc --build tsconfig.commonjs.json",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielfu/solidity-math.git"
  },
  "keywords": [
    "solidity"
  ],
  "author": "gabrielfu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielfu/solidity-math/issues"
  },
  "homepage": "https://github.com/gabrielfu/solidity-math",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@types/node": "^14.11.2",
    "eslint": "^7.17.0",
    "hardhat": "^2.12.6",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0"
  },
  "dependencies": {
    "bn.js": "^5.2.1"
  }
}